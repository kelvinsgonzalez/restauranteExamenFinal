{
  "name": "backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node dist/main.js",
    "start:dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only --ignore-watch node_modules --project tsconfig.json src/main.ts",
    "start:debug": "node --inspect-brk -r ts-node/register src/main.ts",
    "build": "rimraf dist && nest build",
    "lint": "eslint \"src/**/*.ts\" --max-warnings=0",
    "test": "jest",
    "test:e2e": "jest --config test/jest-e2e.json",
    "migrate:run": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:run -d src/database/data-source.ts",
    "migrate:revert": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:revert -d src/database/data-source.ts",
    "migrate:generate": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:generate src/database/migrations/$(date +%s) -d src/database/data-source.ts",
    "seed": "ts-node -r tsconfig-paths/register src/database/seed.ts",
    "migrate:run:prod": "node node_modules/typeorm/cli.js migration:run -d dist/database/data-source.js",
    "seed:prod": "node dist/database/seed.js"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.7",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.7",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.4",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.7",
    "@nestjs/platform-socket.io": "^10.4.7",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/typeorm": "^10.0.1",
    "@nestjs/websockets": "^10.4.7",
    "@socket.io/redis-adapter": "^8.2.1",
    "arg": "^5.0.2",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "glob": "^11.0.0",
    "lodash.includes": "^4.3.0",
    "luxon": "^3.5.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.13.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^5.0.10",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.7",
    "@nestjs/schematics": "^10.1.2",
    "@nestjs/testing": "^10.4.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.14",
    "@types/cookie-parser": "^1.4.5",
    "@types/luxon": "^3.4.2",
    "@types/lodash.includes": "^4.3.7",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.7.4",
    "@types/node-cron": "^3.0.10",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.2"
  }
}
